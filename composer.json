{
    "name": "flipak180/pkrt",
    "type": "library",
    "description": "test task",
    "keywords": [
        "flipak180",
        "pkrt"
    ],
    "homepage": "https://github.com/flipak180/pkrt",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita",
            "email": "flipak180@mail.ru",
            "homepage": "https://github.com/flipak180",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "flipak180\\pkrt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "flipak180\\pkrt\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
